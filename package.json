{
  "name": "social-proof-workshop",
  "version": "1.0.0",
  "description": "ZTM Workshop: Designer to Developer Handoff",
  "main": "index.js",
  "scripts": {
    "start": "sass sass/main.scss css/style.css -w",
    "test:sass": "sass sass/main.scss css/style.css -s compressed",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css -s compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "KB Ruado",
  "license": "ISC",
  "keywords": [],
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "sass": "^1.49.9"
  }
}
